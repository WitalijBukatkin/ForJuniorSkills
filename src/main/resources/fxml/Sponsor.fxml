<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox prefHeight="388.0" prefWidth="573.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SponsorController">
   <VBox spacing="5.0">
      <Label text="name" />
      <TextField fx:id="name" />
      <Label text="description" />
      <TextArea fx:id="description" />
      <Label text="logo" />
      <Button fx:id="logoView" onAction="#chooseLogo" text="choose" />
      <ImageView fx:id="logo" fitHeight="200.0" fitWidth="200.0" />
      <Button onAction="#apply" text="Apply" />
      <Button onAction="#close" text="Cancel" />
   </VBox>
   <VBox>
      <Label text="juniors" />
      <ListView fx:id="juniors" />
   </VBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>